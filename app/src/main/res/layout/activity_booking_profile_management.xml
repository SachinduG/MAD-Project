<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/carparking123"
    tools:context=".BookingProfileManagement">


    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/outlineEdit1"
        android:layout_width="413dp"
        android:layout_height="0dp"
        android:layout_marginTop="71dp"
        android:layout_marginBottom="396dp"
        android:hint="@string/BOOkname"
        android:padding="30dp"
        app:counterEnabled="true"
        app:counterMaxLength="20"
        app:helperText=" email"
        app:layout_constraintBottom_toTopOf="@+id/delete"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/EDITBnamee"
            style="@style/MaterialAlertDialog.MaterialComponents.Title.Text.CenterStacked"
            android:layout_width="347dp"

            android:layout_height="66dp"
            android:background="@drawable/rounded_corner" />



    </com.google.android.material.textfield.TextInputLayout>


    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/outlineEdit2"
        android:layout_width="413dp"
        android:layout_height="160dp"
        android:layout_marginBottom="372dp"
        android:hint="@string/BOOKemail"
        android:padding="30dp"
        app:counterEnabled="true"
        app:counterMaxLength="20"
        app:helperText=" email"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/EDITBEmail"
            style="@style/MaterialAlertDialog.MaterialComponents.Title.Text.CenterStacked"
            android:layout_width="347dp"

            android:layout_height="66dp"
            android:background="@drawable/rounded_corner" />



    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/outlineEdit3"
        android:layout_width="413dp"
        android:layout_height="160dp"
        android:layout_marginBottom="248dp"
        android:hint="@string/BOOKNIC"
        android:padding="30dp"
        app:counterEnabled="true"
        app:counterMaxLength="20"
        app:helperText=" email"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/EDITBNic"
            style="@style/MaterialAlertDialog.MaterialComponents.Title.Text.CenterStacked"

            android:layout_width="347dp"
            android:layout_height="66dp"
            android:background="@drawable/rounded_corner" />


    </com.google.android.material.textfield.TextInputLayout>


    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/outlineEdit4"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="413dp"
        android:layout_height="160dp"
        android:layout_marginBottom="124dp"
        android:hint="@string/BOOKmobile"
        android:padding="30dp"
        app:counterEnabled="true"
        app:counterMaxLength="20"
        app:helperText=" email"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/Editmobile"
            style="@style/MaterialAlertDialog.MaterialComponents.Title.Text.CenterStacked"
            android:layout_width="347dp"
            android:layout_height="66dp"
            android:background="@drawable/rounded_corner" />


    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/EDitUpdatebtn"
        style="@style/Widget.MaterialComponents.ActionBar.Solid"
        android:layout_width="108dp"
        android:layout_height="52dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="32dp"
        android:text="@string/BUpdate"
        app:backgroundTint="#450CF1"
        app:layout_constraintEnd_toStartOf="@+id/delete"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/outlineEdit4" />

    <TextView
        android:id="@+id/EditUpdateprofile"
        android:layout_width="310dp"
        android:layout_height="49dp"
        android:layout_marginTop="32dp"
        android:text="Booking Profile"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/delete"
        android:layout_width="104dp"
        android:layout_height="51dp"
        android:text="@string/Bdelete"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.833"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.939" />


</androidx.constraintlayout.widget.ConstraintLayout>
