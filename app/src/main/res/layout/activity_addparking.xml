<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Addparking">

    <TextView
        android:id="@+id/tpoic1"
        android:layout_width="382dp"
        android:layout_height="53dp"
        android:fontFamily="monospace"
        android:text="@string/addparkingHeader"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#1B0101"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.407"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="96dp"
        android:layout_height="108dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="303dp"
        android:layout_marginBottom="535dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_baseline_add_photo_alternate_24" />

    <Button
        android:id="@+id/btnChoose"
        android:layout_width="243dp"
        android:layout_height="53dp"
        android:layout_alignBottom="@+id/imageView"
        android:layout_marginStart="141dp"
        android:layout_marginTop="110dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="568dp"
        android:layout_toEndOf="@+id/imageView"
        android:text="@string/CHOOSE_IMAGE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/emid"
        android:layout_width="342dp"
        android:layout_height="47dp"
        android:layout_marginTop="215dp"
        android:layout_marginBottom="464dp"
        android:background="@android:color/background_light"
        android:ems="10"
        android:hint="@string/addparkingEmail"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:textColorHint="#120101"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/townid"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/townid"
        android:layout_width="346dp"
        android:layout_height="46dp"
        android:layout_marginTop="283dp"
        android:layout_marginBottom="386dp"
        android:autofillHints=""
        android:background="@color/common_google_signin_btn_text_dark_pressed"
        android:ems="10"
        android:hint="@string/addparkingTown"
        android:inputType="textPersonName"
        android:textColorHint="#0C0000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/addid"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/addid"
        android:layout_width="347dp"
        android:layout_height="45dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="352dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="334dp"
        android:autofillHints=""
        android:background="@color/common_google_signin_btn_text_dark_default"
        android:ems="10"
        android:hint="@string/addparkingAddress"
        android:inputType="textPersonName"
        android:textColorHint="#170101"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>













