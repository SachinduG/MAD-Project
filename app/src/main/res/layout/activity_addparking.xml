<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/car1"
    tools:context=".Addparking">

    <TextView
        android:id="@+id/tpoic1"
        android:layout_width="382dp"
        android:layout_height="53dp"
        android:fontFamily="monospace"
        android:text="@string/addparkingHeader"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#1B0101"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024" />

    <EditText
        android:id="@+id/emid"
        android:layout_width="342dp"
        android:layout_height="47dp"
        android:layout_marginBottom="564dp"
        android:background="@android:color/background_light"
        android:ems="10"
        android:hint="@string/addemail"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:textColorHint="#120101"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/townid"
        android:layout_width="346dp"
        android:layout_height="46dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="386dp"
        android:autofillHints=""
        android:background="@color/common_google_signin_btn_text_dark_pressed"
        android:ems="10"
        android:hint="@string/addparkingTown"
        android:inputType="textPersonName"
        android:textColorHint="#0C0000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/emid"
        app:layout_constraintTop_toBottomOf="@+id/emid"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/addid"
        android:layout_width="347dp"
        android:layout_height="45dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="334dp"
        android:autofillHints=""
        android:background="@color/common_google_signin_btn_text_dark_default"
        android:ems="10"
        android:hint="@string/addparkingAddress"
        android:inputType="textPersonName"
        android:textColorHint="#170101"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/townid"
        app:layout_constraintTop_toBottomOf="@+id/townid"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txtaddid"
        android:layout_width="328dp"
        android:layout_height="46dp"
        android:background="@color/common_google_signin_btn_text_dark_default"
        android:text="@string/addrate_per_hour_100_00_rs"
        android:textAllCaps="false"
        android:textColor="#0B0000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tpoic1"
        app:layout_constraintVertical_bias="0.621" />

    <EditText
        android:id="@+id/mobile"
        android:layout_width="347dp"
        android:layout_height="45dp"
        android:background="@color/common_google_signin_btn_text_dark_default"
        android:ems="10"
        android:hint="@string/parkmobile"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.539"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="268dp"
        android:layout_height="63dp"
        android:layout_marginTop="388dp"
        android:background="@drawable/btn_drawable_selector3"
        android:text="@string/addpark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/emid"
        app:layout_constraintVertical_bias="0.593" />

    <EditText
        android:id="@+id/descrip"
        android:layout_width="335dp"
        android:layout_height="50dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="501dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="180dp"
        android:background="@color/common_google_signin_btn_text_dark_default"
        android:ems="10"
        android:hint="@string/description"
        android:inputType="textMultiLine|text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>












